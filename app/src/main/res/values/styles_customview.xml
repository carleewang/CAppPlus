<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="SwitchButton">
        <attr name="bmWidth" format="dimension"></attr>
        <attr name="bmHeight" format="dimension"></attr>
    </declare-styleable>

    <declare-styleable name="CustomTitleBar">
        <attr name="title_background_color" format="reference|integer"/>
        <attr name="left_visible" format="boolean"/>
        <attr name="right_visible" format="boolean"/>
        <attr name="left_show_mode" format="enum">
            <enum name="all" value="0"></enum>
            <enum name="text" value="1"></enum>
            <enum name="image" value="2"></enum>
        </attr>
        <attr name="right_show_mode" format="enum">
            <enum name="all" value="0"></enum>
            <enum name="text" value="1"></enum>
            <enum name="image" value="2"></enum>
        </attr>
        <attr name="title_text" format="string"/>
        <attr name="title_text_color" format="color"/>
        <attr name="right_text" format="string"/>
        <attr name="right_text_color" format="color"/>
        <attr name="right_drawable" format="reference"/>
        <attr name="left_text" format="string"/>
        <attr name="left_text_color" format="color"/>
        <attr name="left_drawable" format="reference"/>

        <attr name="search_text" format="string"/>
        <attr name="search_hint_text" format="string"/>
    </declare-styleable>

</resources>