<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/matchlive_details"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:background="@drawable/background_layout_race_info_detial"
    android:orientation="vertical"
    android:padding="8dp">

    <TextView
        android:layout_marginBottom="8dp"
        android:layout_marginLeft="8dp"
        android:id="@+id/tv_jige_huiyuanpenghao"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="会员棚号:null"
         />

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content">

        <TextView
            android:id="@+id/tv_jige_saigeshijian"
            android:layout_width="match_parent"
            android:layout_marginBottom="8dp"
            android:layout_marginLeft="8dp"
            android:layout_height="wrap_content"
            android:text="集鸽时间:null"
            />
    </RelativeLayout>



    <TextView
        android:id="@+id/tv_jige_dengjizuobiao"
        android:layout_width="match_parent"
        android:layout_marginBottom="8dp"
        android:layout_marginLeft="8dp"
        android:layout_height="wrap_content"
        android:text="登记坐标:null"
         />

    <TextView
        android:layout_marginBottom="8dp"
        android:layout_marginLeft="8dp"
        android:id="@+id/tv_jige_chazubaodao"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="插组报到:"
         />

    <include layout="@layout/include_live_person_foot_function_layout"/>

</LinearLayout>